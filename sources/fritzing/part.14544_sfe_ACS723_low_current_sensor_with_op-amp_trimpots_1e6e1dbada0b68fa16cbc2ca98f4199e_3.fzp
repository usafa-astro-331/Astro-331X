<module fritzingVersion="0.5.2b.02.18.4756" moduleId="14544_sfe_ACS723_low_current_sensor_with_op-amp_trimpots_1e6e1dbada0b68fa16cbc2ca98f4199e_3" referenceFile="ACS723_Low_Current_Sensor.fzp">
