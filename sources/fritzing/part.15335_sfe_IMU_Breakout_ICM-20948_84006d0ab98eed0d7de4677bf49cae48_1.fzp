<module fritzingVersion="0.5.2b.02.18.4756" moduleId="15335_sfe_IMU_Breakout_ICM-20948_84006d0ab98eed0d7de4677bf49cae48_1" referenceFile="SparkFun_IMU_Breakout_ICM-20948.fzp">
